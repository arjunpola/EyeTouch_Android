<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/eyetouch_tab_bg"
    android:orientation="vertical">
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:gravity="center_vertical"
        android:baselineAligned="false"
        android:layout_weight="0.3" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="@null"
                android:contentDescription="@string/CD_history"
                android:onClick="subNav"
                android:scaleX="0.6"
                android:scaleY="0.6"
                android:src="@drawable/history_tab_n" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >
            
                    <ImageButton
            android:id="@+id/drugs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:contentDescription="@string/CD_drugs"
            android:onClick="subNav"
            android:scaleX="0.6"
            android:scaleY="0.6"
            android:src="@drawable/drugs_tab_n" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >
            
        <ImageButton
            android:id="@+id/classifications"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:contentDescription="@string/CD_classifications"
            android:onClick="subNav"
            android:scaleX="0.6"
            android:scaleY="0.6"
            android:src="@drawable/classifications_tab_n" />
          
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="0dp"
        android:gravity="center_vertical"
        android:baselineAligned="false"
         android:layout_weight="0.3" >
        
     <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/formulae"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/formulae"
                android:background="@null"
                android:contentDescription="@string/CD_formulae"
                android:onClick="detailNav"
                android:scaleX="0.6"
            	android:scaleY="0.6"
                android:src="@drawable/formulae_tab_n" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/clinicalTests"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="@null"
                android:contentDescription="@string/CD_clinicalT"
                android:onClick="detailNav"
                android:scaleX="0.6"
            	android:scaleY="0.6"
                android:src="@drawable/clinical_tests_tab_n" />
            
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

                    <ImageButton
            android:id="@+id/guestArticles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:contentDescription="@string/CD_guestA"
            android:onClick="subNav"
            android:scaleX="0.6"
            android:scaleY="0.6"
            android:src="@drawable/guest_articles_tab_n" />
          
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="0dp"
        android:gravity="center_vertical"
        android:baselineAligned="false"
         android:layout_weight="0.3" >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/report"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="@null"
                android:contentDescription="@string/CD_report"
                android:onClick="detailNav"
                android:scaleX="0.6"
            	android:scaleY="0.6"
                android:src="@drawable/report_tab_n" />
            
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/disclaimer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="@null"
                android:contentDescription="@string/CD_media"
                android:onClick="detailNav"
                android:scaleX="0.6"
            	android:scaleY="0.6"
                android:src="@drawable/media_tab_n" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center" >

            <ImageButton
                android:id="@+id/meetTheTeam"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:background="@null"
                android:contentDescription="@string/CD_meetTH"
                android:onClick="theTeam"
                android:scaleX="0.6"
            	android:scaleY="0.6"
                android:src="@drawable/the_team_tab_n" />
                
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>